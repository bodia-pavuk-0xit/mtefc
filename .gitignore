
# Ignore directories generated by Composer
/vendor/
/composer.lock

# Ignore environment-specific files
/sites/*/settings.php
/sites/*/services.yml
/sites/*/development.services.yml

# Ignore Drupal's private files directory
/private/

# Ignore backups and config files
/sites/default/files/config_*

# Ignore DDEV files
.ddev/

# Ignore Drupal's temporary files directory
/tmp/

# Ignore database files
*.sql
*.sqlite

# Ignore log files and error logs
*.log
error_log

# Ignore compiled PHP files
*.php~
*.swp

# Ignore node_modules if using Node.js
/node_modules/

# Ignore Sass cache files
.sass-cache/

/css/

# Ignore user-specific and IDE files
*.sublime-project
*.sublime-workspace
.idea/
*.iml

# Ignore files generated by running Drupal tests
/simpletest

# Ignore custom .env files
.env
